        ]
      },
      {
        "name": "edgeHover",
        "value": null,
        "on": [
          {
            "events": "@edgeMark:mouseover",
            "update": "datum.linkId"
          },
          {
            "events": "@edgeMark:mouseout",
            "update": "null"
          }
        ]
      },
      {
        "name": "selectedNode",
        "value": null,
        "on": [
